<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.brandslam.threeisummit">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/applogo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        <!-- Activity Declarations -->
        <activity
            android:name=".AppLoadingActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegistrationActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
        <activity
            android:name=".HomescreenActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
        <activity android:name=".InviteActivity" />
        <activity
            android:name=".FeedbackActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
        <activity android:name=".TestimonialActivity" />
        <activity android:name=".GuestActivity" />
        <activity android:name=".TestimonialAddActivity" />
        <activity
            android:name=".GatePassActivity"
            android:label="@string/title_activity_gate_pass"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />




        <activity android:name=".GuestCategoryActivity" />
        <activity android:name=".GalleryActivity" />
        <activity android:name=".DhruvActivity" />
        <activity android:name=".ClassRoomOneActivity" />
        <activity android:name=".ClassRoomTwoActivity" />
        <activity android:name=".YoutubeActivity"></activity>
        <activity android:name="com.brandslam.threeisummit.GuestCategoryActivity" />
        <activity android:name="com.brandslam.threeisummit.GalleryActivity" />
        <activity android:name="com.brandslam.threeisummit.DhruvActivity" />
        <activity android:name="com.brandslam.threeisummit.ClassRoomOneActivity" />
        <activity android:name="com.brandslam.threeisummit.ClassRoomTwoActivity"/>

    </application>

</manifest>